{
  "globals": {
    "__PATH_PREFIX__": true
  },
  "extends": "airbnb",
  "rules": {
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "semi": [
      2,
      "never"
    ],
    "import/prefer-default-export": 0,
    "arrow-parens": 0,
    "no-nested-ternary": 0,
    "react/no-danger": 0,
    "react/jsx-props-no-spreading": 0,
  }
}